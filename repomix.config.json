{
  "output": {
    "filePath": "_fab-errors.xml",
    "style": "xml",
    "parsableStyle": false,
    "fileSummary": true,
    "directoryStructure": true,
    "removeComments": false,
    "removeEmptyLines": false,
    "topFilesLength": 5,
    "showLineNumbers": false,
    "copyToClipboard": false
  },
  "include": [
  ],
  "ignore": {
    "useGitignore": true,
    "useDefaultPatterns": true,
    "customPatterns": [
      "data",
      "_test-results.json",
//      "docs/",
//      "docs/RULEZZ.md",
//      "src/",
//      "test/",
      "logs",
      ".idea",
      ".run",
      ".aider",
      "*.patch",
      "*.log",
      "LICENSE",
      "log.txt",
      "all-files*.*",
      "repomix*.*"
    ]
  },
  "security": {
    "enableSecurityCheck": true
  },
  "tokenCount": {
    "encoding": "o200k_base"
  }
}